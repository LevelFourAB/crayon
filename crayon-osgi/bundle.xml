<bundle>
	<name>Crayon OSGi</name>
	<symbolicName>se.l4.crayon.osgi</symbolicName>
	<version>0.3.0</version>

	<website>http://crayon-java.googlecode.com</website>

	<description>
		OSGi compatibility layer for Crayon.
	</description>

	<requires>
		<package name="*" />
	</requires>

	<provides>
		<package name="se.l4.crayon.osgi.internal" private="true" />
		<package name="se.l4.crayon.osgi" version="${version}" />
	</provides>
</bundle>
